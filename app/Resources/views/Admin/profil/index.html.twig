<script>
    $(function () {
        $('#myTable').DataTable();


        $('#example23').DataTable({
            paging: true,
            searching: true,
            destroy: true,
            dom: 'Bfrtip',
            buttons: [
                'copy', 'csv', 'excel', 'pdf', 'print'
            ]
        });
        $('.buttons-copy, .buttons-csv, .buttons-print, .buttons-pdf, .buttons-excel').addClass('btn btn-primary mr-1');
    });

</script>
















{% endblock %}
